    "from datetime import date, timedelta\n",
    "\n",
    "year, month, day = (map(int, input().split(' ')))\n",
    "day_raz = int(input())\n",
    "\n",
    "data = date(year, month, day)\n",
    "new_date = data + timedelta(day_raz)\n",
    "\n",
    "\n",
    "print(new_date)"
